<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.andylx96.sensorapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity" />
        <activity android:name=".ProximitySensor" />
        <activity android:name=".Accelerometer" />
        <activity android:name=".Gyroscopic" />
        <activity android:name=".Pressure" />
        <activity android:name=".Magnetometer" />
        <activity android:name=".StepCounter"></activity>
    </application>

</manifest>